    xmlns:local="using:MyGameApp"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d">

    <Grid Background="#222">
        <Button x:Name="button" Content="Play Game" HorizontalAlignment="left" Margin="101,138,0,0" VerticalAlignment="Center" Background="LightSalmon" Height="64" Width="136" Tapped="Go_To_Game"/>
        <TextBlock x:Name="textBlock" HorizontalAlignment="Left" Margin="15,124,0,0" TextWrapping="Wrap" Text="Robot Mine Game" VerticalAlignment="Top" Height="68" Width="284" Foreground="LightSalmon" FontSize="26" TextAlignment="Center"/>
       
    </Grid>


</Page>
